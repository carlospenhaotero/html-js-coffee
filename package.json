{
  "name": "html-js-coffee-cu",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "anvil": "anvil --load-state fundme-anvil.json --block-time 5",
    "format": "prettier --write .",
    "compile": "tsc --project tsconfig.json",
    "start": "concurrently -k \"pnpm anvil\" \"pnpm dev\"",
    "stop": "kill-port 8545 5173",
    "restart": "pnpm stop && pnpm start"
  },
  "dependencies": {
    "viem": "^2.23.2"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "concurrently": "^9.0.0",
    "kill-port": "^2.0.1"
  }
}
